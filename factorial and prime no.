        "#prime number\n",
        "n = int(input())\n",
        "while n>1:\n",
        "  if n==2:\n",
        "    print(\"prime\")\n",
        "    break\n",
        "  elif n%2==0:\n",
        "    print(\"not prime\")\n",
        "    break\n",
        "  else:\n",
        "    print(\"prime\")\n",
        "    break"
